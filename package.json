  "description": "ESLint Shareable Config for JavaScript and React",
  "author": "Mike Skaife <mike@skaife.me>",
  "contributors": [
    {
      "name": "Mike Skaife",
      "url": "https://twitter.com/mikeskaife",
      "email": "mike@skaife.me"
    }
  ],
  "license": "MIT",
  "homepage": "https://github.com/mikeskaife/eslint-config-react-skoof#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikeskaife/eslint-config-react-skoof.git"
  },
  "bugs": {
    "url": "https://github.com/mikeskaife/eslint-config-react-skoof/issues"
  },
  "main": "index.js",
  "scripts": {
    "lint": "eslint './rules/**/*.js' './tests/**/*.test.js'",
    "lint:verbose": "DEBUG=eslint:cli-engine eslint .",
    "test": "mocha --require babel-core/register ./tests/**/*.test.js",
    "cover": "istanbul cover --dir ./coverage _mocha -- --compilers js:babel-core/register --colors -R spec ./tests/**/*.test.js"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "1.6.0",
    "babel-core": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "chai": "4.1.2",
    "coveralls": "3.0.1",
    "eslint": "5.16.0",
    "eslint-find-rules": "3.3.1",
    "eslint-plugin-import": "2.17.1",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "5.0.4"
  },
  "peerDependencies": {
    "eslint": "5.16.0",
    "eslint-find-rules": "3.3.1",
    "eslint-plugin-import": "2.17.1",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "javascript",
    "typescript",
    "react",
    "reacta11y",
    "styleguide"
